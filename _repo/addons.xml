<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.tamilstreamer" name="TamilStreamer" version="1.0.0" provider-name="Dilip">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.xbmcswift2" version="2.4.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language></language>
    <summary>Summary for "TamilStreamer"</summary>
    <description>Description for "TamilStreamer"</description>
  </extension>
</addon>

    <addon id="repository.tamilrepo" name="Tamil Repo" version="1.0.0" provider-name="Dilip">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Tamil Repo">
            <info compressed="false">http://tamilrepokodi.site88.net/_repo/addons.xml</info>
            <checksum>http://tamilrepokodi.site88.net/_repo/addons.xml.md5</checksum>
            <datadir zip="true">http://tamilrepokodi.site88.net/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>All Tamil addons for KODI</summary>
            <description>Here you can find all tamil addons for KODI</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
